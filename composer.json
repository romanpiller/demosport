{
	"name": "romanpiller/demosport",
	"description": "demosport",
	"keywords": ["nette", "sport"],
	"type": "project",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Roman Piller",
			"email": "roman.piller@gmail.com"
		}
	],
	"require": {
		"php": "^8.2",
		"nette/application": "^3.1.8",
		"nette/bootstrap": "^3.1.3",
		"nette/caching": "^3.1.4",
		"nette/database": "^3.1.6",
		"nette/di": "^3.0.14",
		"nette/finder": "^2.5.4",
		"nette/forms": "^3.1.8",
		"nette/http": "^3.2.1",
		"nette/mail": "^3.1.9",
		"nette/robot-loader": "^3.4.1",
		"nette/security": "^3.1.6",
		"nette/utils": "^3.2.8",
		"latte/latte": "^3.0.4",
		"tracy/tracy": "^2.9.5"
	},
	"require-dev": {
		"nette/tester": "^2.4.3",
		"symfony/thanks": "^1.2.10",
		"phpstan/phpstan": "^1.9.2"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": false
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse --memory-limit=400M --xdebug",
		"test": "tester tests -s -o console -C --colors 1"
	}
}
